{
  "name": "release",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/release/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/release",
        "main": "packages/release/src/index.ts",
        "tsConfig": "packages/release/tsconfig.lib.json",
        "assets": ["packages/release/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/release/jest.config.ts"
      }
    },
    "version": {
      "executor": "@semantic-versioning/release:version",
      "cache": true,
      "options": {},
      "configurations": {
        "production": {
          "dryRun": false
        },
        "development": {
          "dryRun": true,
          "verbose": true
        }
      },
      "defaultConfiguration": "development"
    }
  },
  "tags": []
}
